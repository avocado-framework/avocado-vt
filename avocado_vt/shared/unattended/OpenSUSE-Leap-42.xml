      <script>
        <debug config:type="boolean">true</debug>
        <filename>config</filename>
        <source><![CDATA[!/bin/bash
###Enable ssh service when it is start
chkconfig sshd on
sed -i -e 's/\(PasswordAuthentication\s\)no/\1yes/g'  /etc/ssh/sshd_config
service sshd restart
#Since guest will perform system initialization at first boot after installation, so it's better to run a basic test to make following script to take effect.
# generate ethernet interface configuration template
cat > /etc/sysconfig/network/.ifcfg-ethx <<eof
BOOTPROTO='dhcp'
STARTMODE='auto'
USERCONTROL='no'
eof

# auto generate ethernet interface configuration
echo '''interfaces=$(ls /sys/class/net)
net_dir="/etc/sysconfig/network"
net_temp="$net_dir/.ifcfg-ethx"
for iface in $interfaces; do
    conf="$net_dir/ifcfg-$iface"
    test -f "$conf" || cp $net_temp $conf
done
''' >> /etc/init.d/boot.local
]]></source>
      </script>
    </post-scripts>
